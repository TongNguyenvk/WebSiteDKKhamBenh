},
{
"name": "Update Code",
"request": {"method": "PUT", "header": [{"key": "Content-Type", "value": "application/json"},{"key": "Authorization", "value": "Bearer {{token}}"}], "body": {"mode": "raw", "raw": "{\"valueVi\":\"Updated Vi\",\"valueEn\":\"Updated En\"}"}, "url": "{{baseUrl}}/allcode/TEST1"}
},
{
"name": "Delete Code",
"request": {"method": "DELETE", "header": [{"key": "Authorization", "value": "Bearer {{token}}"}], "url": "{{baseUrl}}/allcode/TEST1"}
}
]
}
]
}
